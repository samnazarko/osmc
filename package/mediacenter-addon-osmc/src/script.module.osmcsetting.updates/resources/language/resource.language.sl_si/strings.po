msgid "OSMC"
msgstr ""

msgctxt "#32137"
msgid "Update not permitted."
msgstr "Posodobitev ni dovoljena."

msgctxt "#32138"
msgid "Unable to reach internet."
msgstr "Ni mogoče doseči interneta."

