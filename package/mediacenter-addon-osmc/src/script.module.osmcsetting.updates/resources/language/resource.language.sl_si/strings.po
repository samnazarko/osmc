msgstr "Vašemu sistemu zmanjkuje prostora za shranjevanje (ostalo <%sMB)."

msgctxt "#32143"
msgid "Would you like to try and clear unused system files?"
msgstr "Bi radi poskusili počistiti neuporabljene sistemske datoteke?"

msgctxt "#32144"
msgid "Try rebooting a couple times to clear out temporary files."
msgstr "Poskusite nekajkrat znova zagnati, da počistite začasne datoteke."

msgctxt "#32145"
msgid "Restore failed"
msgstr "Obnovitev ni uspela"

msgctxt "#32146"
msgid "Failure to reading the file."
msgstr "Napaka pri branju datoteke."

msgctxt "#32147"
msgid "OSMC Backup"
msgstr "Varnostna kopija OSMC"

msgctxt "#32148"
msgid "Location for backup not provided."
msgstr "Lokacija za varnostno kopiranje ni na voljo."

msgctxt "#32149"
msgid "Set the backup folder in My OSMC."
msgstr "Nastavitev poti mape varnostne kopije v moj OSMC."

msgctxt "#32150"
msgid "Insufficent diskspace at target location."
msgstr "Na ciljni lokaciji ni dovolj prostora na disku."

msgctxt "#32151"
msgid "Backup location not writeable."
msgstr "Lokacija varnostne kopije ni zapisljiva."

msgctxt "#32152"
msgid "Backup failed"
msgstr "Varnostno kopiranje ni uspelo"

msgctxt "#32153"
msgid "Backup failed to copy the tar file."
msgstr "Varnostna kopija ni uspela kopirati datoteke tar."

msgctxt "#32154"
msgid "Select a backup file"
msgstr "Izberite datoteko varnostne kopije"

msgctxt "#32155"
msgid "Browse to backup file"
msgstr "Poiščite varnostno kopijo datoteke"

msgctxt "#32156"
